{
  "name": "osc-blog-publish",
  "displayName": "osc-blog-publish",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.46.0"
  },
  "publisher": "someone",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:osc-blog-publish.helloWorld",
    "onLanguage:markdown"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "osc-blog-publish.helloWorld",
        "title": "Hello World"
      }
    ],
    "configuration": {
      "title": "",
      "properties": {
        "osc-blog-publish.cookie": {
          "type": "string",
          "default": "cookie",
          "description": "osc cookie"
        },
        "osc-blog-publish.showTip": {
          "type": "boolean",
          "default": true,
          "description": "启动时显示自定义欢迎页"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.46.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
